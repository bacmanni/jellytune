<?xml version="1.0" encoding="utf-8"?>
<interface>
    <object class="AdwDialog" id="_root">
        <property name="content-height">440</property>
        <property name="content-width">400</property>
        <child>
            <object class="GtkBox">
                <property name="orientation">1</property>
                <child>
                    <object class="AdwSpinner" id="_spinner">
                        <property name="margin-top">46</property>
                        <property name="halign">3</property>
                        <property name="valign">3</property>
                        <property name="vexpand">true</property>
                        <property name="hexpand">true</property>
                        <property name="width-request">48</property>
                        <property name="height-request">48</property>
                    </object>
                </child>
                <child>
                    <object class="AdwToolbarView" id="_results">
                        <child type="top">
                            <object class="AdwHeaderBar">
                                <child>
                                    <object class="GtkBox">
                                        <property name="margin-top">6</property>
                                        <property name="margin-bottom">6</property>
                                        <property name="margin-start">12</property>
                                        <property name="margin-end">12</property>
                                        <child>
                                            <object class="GtkImage" id="_albumArt">
                                                <property name="overflow">hidden</property>
                                                <style>
                                                    <class name="icon-dropshadow"/>
                                                    <class name="album-art-small"/>
                                                </style>
                                                <property name="pixel-size">36</property>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="GtkBox">
                                                <property name="orientation">1</property>
                                                <property name="margin-top">6</property>
                                                <property name="margin-start">12</property>
                                                <child>
                                                    <object class="GtkLabel" id="_track">
                                                        <property name="ellipsize">3</property>
                                                        <property name="justify">0</property>
                                                        <property name="halign">1</property>
                                                        <style>
                                                            <class name="title-4"/>
                                                        </style>
                                                    </object>
                                                </child>
                                                <child>
                                                    <object class="GtkLabel" id="_artist">
                                                        <property name="ellipsize">3</property>
                                                        <property name="justify">0</property>
                                                        <property name="halign">1</property>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                    </object>
                                </child>
                            </object>
                        </child>
                        <child>
                            <object class="GtkScrolledWindow">
                                <property name="hscrollbar-policy">2</property>
                                <property name="propagate-natural-height">true</property>
                                <property name="propagate-natural-width">true</property>
                                <child>
                                    <object class="GtkBox">
                                        <property name="margin-bottom">12</property>
                                        <property name="margin-start">24</property>
                                        <property name="margin-end">24</property>
                                        <property name="orientation">1</property>
                                        <child>
                                            <object class="GtkBox">
                                                <property name="orientation">1</property>
                                                <child>
                                                    <object class="GtkLabel" id="_lyrics">
                                                        <property name="wrap">true</property>
                                                        <property name="margin-top">12</property>
                                                        <property name="halign">1</property>
                                                    </object>
                                                </child>
                                                <child>
                                                    <object class="GtkButton" id="_update">
                                                        <property name="visible">false</property>
                                                        <property name="label">Update</property>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>